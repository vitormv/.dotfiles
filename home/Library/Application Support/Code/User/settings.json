{
  "editor.fontSize": 14,
  "editor.rulers": [{}, 80],
  "workbench.tree.indent": 18,
  "breadcrumbs.enabled": true,
  "editor.formatOnPaste": true,
  "editor.tabSize": 2,
  "editor.autoIndent": "full",
  "workbench.sideBar.location": "left",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "workbench.editor.highlightModifiedTabs": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.autoClosingQuotes": "always",
  "window.openFoldersInNewWindow": "on",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "\"MesloLGM Nerd Font\"",
  "terminal.integrated.fontSize": 14,
  "workbench.colorCustomizations": {
    "terminal.background": "#181818",
    "terminal.foreground": "#D8D8D8",
    "terminalCursor.background": "#D8D8D8",
    "terminalCursor.foreground": "#D8D8D8",
    "terminal.ansiBlack": "#181818",
    "terminal.ansiBlue": "#7CAFC2",
    "terminal.ansiBrightBlack": "#585858",
    "terminal.ansiBrightBlue": "#7CAFC2",
    "terminal.ansiBrightCyan": "#86C1B9",
    "terminal.ansiBrightGreen": "#A1B56C",
    "terminal.ansiBrightMagenta": "#BA8BAF",
    "terminal.ansiBrightRed": "#AB4642",
    "terminal.ansiBrightWhite": "#F8F8F8",
    "terminal.ansiBrightYellow": "#F7CA88",
    "terminal.ansiCyan": "#86C1B9",
    "terminal.ansiGreen": "#A1B56C",
    "terminal.ansiMagenta": "#BA8BAF",
    "terminal.ansiRed": "#AB4642",
    "terminal.ansiWhite": "#D8D8D8",
    "terminal.ansiYellow": "#F7CA88"
  },
  "editor.tokenColorCustomizations": {
    "[Monokai Dimmed]": {
      "textMateRules": [
        {
          "scope": "entity.name.type.class",
          "settings": {
            "foreground": "#ce2029"
          }
        },
        {
          "scope": "entity.name.type",
          "settings": {
            "foreground": "#6f4e37"
          }
        },
        {
          "scope": "entity.name.namespace",
          "settings": {
            "foreground": "#FB4934"
          }
        }
      ]
    }
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "workbench.editor.focusRecentEditorAfterClose": false,
  "editor.multiCursorModifier": "alt",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.detectIndentation": false,
  "editor.renderControlCharacters": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
  },
  "typescript.updateImportsOnFileMove.enabled": "never",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordSeparators": "`~!@^&*()=+[{]}\\|;:'\",<>/?"
  },
  "[stylus]": {
    "editor.wordSeparators": "`~!@^&*()=+[{]}\\|;:'\",<>/?"
  },
  "[css]": {
    "editor.wordSeparators": "`~!@^&*()=+[{]}\\|;:'\",<>/?"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?"
  },
  "eslint.format.enable": true,
  "prettier.requireConfig": true,
  "explorer.compactFolders": false,
  "git.confirmSync": false,
  "workbench.editor.wrapTabs": true,
  "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.suggestSelection": "first",
  "editor.bracketPairColorization.enabled": true,
  "telemetry.telemetryLevel": "error",

  "editor.fontFamily": "'Oxygen Mono', Menlo, Monaco, 'Courier New', monospace",
  "git.enableStatusBarSync": false,
  "editor.minimap.maxColumn": 80,
  "extensions.ignoreRecommendations": true,
  "editor.inlineSuggest.enabled": true,
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "history": false,
      "remote": false
    },
    "editorGroup": {
      "blame": true,
      "compare": false
    },
    "editorTab": {
      "clipboard": true,
      "compare": false,
      "history": false,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scm": {
      "authors": true
    },
    "scmGroupInline": {
      "stash": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": false,
      "stash": true
    }
  },
  "gitlens.modes": {
    "zen": {
      "name": "Zen",
      "statusBarItemName": "Zen",
      "description": "for a zen-like experience, disables many visual features",
      "codeLens": false,
      "currentLine": false,
      "hovers": false,
      "statusBar": false
    },
    "review": {
      "name": "Review",
      "statusBarItemName": "Reviewing",
      "description": "for reviewing code, enables many visual features",
      "codeLens": true,
      "currentLine": true,
      "hovers": true
    }
  },
  "terminal.integrated.scrollback": 2000,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
  "workbench.colorTheme": "Monokai Dimmed",
  "editor.stickyScroll.enabled": true,
  "git.openDiffOnClick": false,
  "workbench.iconTheme": "material-icon-theme",
  "gitlens.hovers.currentLine.over": "line",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "typescript.preferences.useAliasesForRenames": false,
  "javascript.preferences.useAliasesForRenames": false,
  "terminal.explorerKind": "external",
  "[properties]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false
  },
  "files.associations": {
    "*.conf": "properties"
  },
  "local-history.path": "/tmp/vscode/localhistory",
  "terminal.integrated.cursorStyle": "line",
  "redhat.telemetry.enabled": false,
  "launch": {
    "configurations": [
      {
        "type": "node",
        "name": "react-app-rewired-jest-test",
        "request": "launch",
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/react-app-rewired",
        "args": ["test", "--runInBand"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "protocol": "inspector",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  },
  "svelte.enable-ts-plugin": true,
  "search.exclude": {
    "vendor/**/*": true,
    ".yarn/": true,
  },
  "todo-tree.general.tags": ["@todo:", "TODO:"],
  "todo-tree.general.tagGroups": {
    "TODO": [
      "TODO:",
      "@todo:",
      "TODO",
    ]
},
"[svelte]": {
  "editor.defaultFormatter": "svelte.svelte-vscode"
},
}
